#0 未做起

#1 4-line  
def wordBreak(self, s, wordDict):
    ok = [True]   # 此算是关键
    for i in range(1, len(s)+1):
        ok += any(ok[j] and s[j:i] in wordDict for j in range(i)),
    return ok[-1]
    
#2 DP法
def word_break(s, words):
 	d = [False] * len(s)    
 	for i in range(len(s)):
 		for w in words:
 			if w == s[i-len(w)+1:i+1] and (d[i-len(w)] or i-len(w) == -1):
 				d[i] = True
 	return d[-1]
